{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "gtxb5/client-render",
  "title": "Me - Client Render",
  "category": "text",
  "icon": "google",
  "description": "Adds a header with a hover effect",
  "keywords": ["header", "underline", "hover"],
  "version": "1",
  "textdomain": "gtxb5",
  "editorScript": "file:./index.js",

  "attributes": {

    "year": {
      "type": "string"
    },

    "country": {
      "type": "string"
    },

    "bgColor": {
      "type": "string",
      "default": "#f87171"
    },

    "txtColor": {
      "type": "string",
      "default": "#fff"
    }

  },
  "style": "file:./index.css"


}